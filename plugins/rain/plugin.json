{
  "id": "rain-effect",
  "name": "雨滴特效",
  "version": "1.0.0",
  "description": "为页面添加逼真的雨滴下落动画效果，支持自定义雨滴密度、速度、颜色等参数",
  "author": "Sentra Auto Browser",
  "category": "visual-effects",
  "tags": ["rain", "雨滴", "water", "水", "weather", "天气", "animation", "动画"],
  "permissions": ["dom-modification", "style-injection", "script-injection"],
  "parameters": [
    {
      "name": "intensity",
      "type": "number",
      "description": "雨滴密度，数值越大雨滴越多",
      "default": 30,
      "min": 1,
      "max": 100,
      "required": false
    },
    {
      "name": "speed",
      "type": "number",
      "description": "雨滴下落速度",
      "default": 1,
      "min": 0.1,
      "max": 3,
      "required": false
    },
    {
      "name": "color",
      "type": "string",
      "description": "雨滴颜色",
      "default": "#4A90E2",
      "required": false
    },
    {
      "name": "opacity",
      "type": "number",
      "description": "雨滴透明度",
      "default": 0.6,
      "min": 0.1,
      "max": 1,
      "required": false
    },
    {
      "name": "angle",
      "type": "number",
      "description": "雨滴下落角度",
      "default": 0,
      "min": -30,
      "max": 30,
      "required": false
    },
    {
      "name": "duration",
      "type": "number",
      "description": "持续时间（秒），0表示永久",
      "default": 0,
      "min": 0,
      "max": 3600,
      "required": false
    }
  ],
  "enabled": true,
  "autoLoad": false,
  "priority": 1
}
