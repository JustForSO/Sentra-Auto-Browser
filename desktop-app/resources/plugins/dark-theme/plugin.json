{
  "id": "dark-theme",
  "name": "暗黑主题切换",
  "version": "1.0.0",
  "description": "为任何网页应用暗黑主题，保护眼睛，提升夜间浏览体验",
  "author": "Sentra Auto Browser",
  "category": "utility",
  "tags": ["dark", "暗黑", "theme", "主题", "night", "夜间", "护眼", "深色"],
  "permissions": ["dom-modification", "style-injection"],
  "parameters": [
    {
      "name": "intensity",
      "type": "number",
      "description": "暗化强度，数值越高越暗",
      "default": 80,
      "min": 10,
      "max": 95,
      "required": false
    },
    {
      "name": "preserveImages",
      "type": "boolean",
      "description": "是否保持图片原始亮度",
      "default": true,
      "required": false
    },
    {
      "name": "accentColor",
      "type": "string",
      "description": "强调色，用于链接和按钮",
      "default": "#4A9EFF",
      "required": false
    },
    {
      "name": "backgroundColor",
      "type": "string",
      "description": "背景色",
      "default": "#1a1a1a",
      "required": false
    },
    {
      "name": "textColor",
      "type": "string",
      "description": "文字颜色",
      "default": "#e0e0e0",
      "required": false
    }
  ],
  "enabled": true,
  "autoLoad": false,
  "priority": 2
}
